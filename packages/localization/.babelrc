{
  "presets": [
    [
      "@babel/preset-env",
      {
        "forceAllTransforms": true,
        "debug": false,
        "modules": false
      }
    ],
    "@babel/preset-flow",
    "@babel/preset-react"
  ],
  "plugins": [
    "transform-flow-strip-types",
    "syntax-dynamic-import",
    "transform-class-properties",
  ]
}